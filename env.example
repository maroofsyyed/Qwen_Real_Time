# Qwen Vision-Language Service Configuration

# Server settings
HOST=0.0.0.0
PORT=8000
HTTPS_ENABLED=true
SSL_CERT_PATH=/path/to/cert.pem
SSL_KEY_PATH=/path/to/key.pem

# Model settings
MODEL_PATH=/root/models/qwen2.5-vl-32b
MODEL_DTYPE=float16
MODEL_MAX_TOKENS=2048
RESPONSE_MAX_TOKENS=128

# vLLM server settings
VLLM_HOST=localhost
VLLM_PORT=12345
VLLM_API_BASE=http://localhost:12345/v1

# Frame processing
FRAME_SAMPLE_RATE=2.0
FRAME_QUEUE_SIZE=8
FRAME_WIDTH=1024
FRAME_HEIGHT=1024

# Security
JWT_SECRET_KEY=CHANGE_THIS_TO_RANDOM_SECRET_KEY
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=60
REQUIRE_AUTH=true

# Safety filters
ENABLE_NSFW_FILTER=true
ENABLE_FACE_BLUR=false

# Performance
MAX_CONCURRENT_SESSIONS=10
GPU_MEMORY_UTILIZATION=0.90

# Logging
LOG_LEVEL=INFO
LOG_FILE=/var/log/qwen-service/app.log

# Analytics
ENABLE_METRICS=true
METRICS_PORT=9090

